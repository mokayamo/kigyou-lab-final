{
  "version": 2,
  "name": "kigyou-lab-v2",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": false
  },
  "alias": ["kigyou-lab.vercel.app"],
  "env": {
    "NODE_ENV": "production",
    "VERCEL_FORCE_NO_BUILD_CACHE": "1",
    "VERCEL_PROJECT_ID": "new"
  }
}