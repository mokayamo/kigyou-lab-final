{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": false
  },
  "alias": ["kigyou-lab.vercel.app"],
  "env": {
    "NODE_ENV": "production"
  }
}